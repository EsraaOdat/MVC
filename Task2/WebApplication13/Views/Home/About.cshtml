@{
    ViewBag.Title = "About";
}
<main aria-labelledby="title">
    <h2 id="title">@ViewBag.Title.</h2>
    <h3>@ViewBag.Message</h3>


    <b>Employee Name:</b> @TempData["EmployeeName"]<br />
    <b>Company Name:</b> @TempData["Company"]<br />
    <b>Address:</b> @TempData["Address"]<br />

    <p>@ViewBag.ReceivedData</p>



    <div>
        @Session["Message"]
    </div>


    <p>Use this area to provide additional information.</p>
</main>